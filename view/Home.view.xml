   <!--Dialog Imprimir Documento-->
    <Dialog title="Impresión de Documento" id="dlg_doc_impresion" class="cabecera_dialogo" stretchOnPhone="true" contentWidth="auto" contentHeight="auto">
        <content>
           
            <f:Form 
            minWidth="1024"
            maxContainerCols="2"
            editable="true">
            <f:title>
                
            </f:title>
            <f:layout>
                <f:ResponsiveGridLayout
                    labelSpanXL="3"
                                    labelSpanL="3"
                                    labelSpanM="3"
                                    labelSpanS="6"
                                    adjustLabelSpan="false"
                                    emptySpanXL="4"
                                    emptySpanL="4"
                                    emptySpanM="4"
                                    emptySpanS="0"
                                    columnsXL="1"
                                    columnsL="1"
                                    columnsM="1"
                                    singleContainerFullSize="false" />
            </f:layout>
            <f:formContainers>
                <f:FormContainer>
                    <f:formElements>
                        <f:FormElement label="Número de Documento">
                            <f:fields>
                                
                                    <SearchField width="100%" placeholder="Ingrese Número de Documento" value="{/imprimirDoc/pNumPedido}" showSearchButton="false"></SearchField>
                                    
                            </f:fields>
                        </f:FormElement>
                    </f:formElements>
                </f:FormContainer>
            </f:formContainers>
        </f:Form>

        <f:Form 
            minWidth="1024"
            maxContainerCols="2"
            editable="true">
            <f:title>
                <core:Title text="Detalle de Impresión" />
            </f:title>
            <f:layout>
                <f:ResponsiveGridLayout
                    labelSpanXL="3"
                                    labelSpanL="3"
                                    labelSpanM="3"
                                    labelSpanS="6"
                                    adjustLabelSpan="false"
                                    emptySpanXL="4"
                                    emptySpanL="4"
                                    emptySpanM="4"
                                    emptySpanS="0"
                                    columnsXL="1"
                                    columnsL="1"
                                    columnsM="1"
                                    singleContainerFullSize="false" />
            </f:layout>
            <f:formContainers>
                <f:FormContainer>
                    <f:formElements>
                        <f:FormElement label="Oferta/Pedido Agrupada" >
                            <f:fields> 
                                    <RadioButton groupName="GroupA" id="opcion1"  selected="true"/>
                            </f:fields>
                        </f:FormElement>

                        <f:FormElement label="Condición de Entregas">
                            <f:fields>
                                    <RadioButton groupName="GroupA" id="opcion2"  />
                            </f:fields>
                        </f:FormElement>

                        <f:FormElement label="Impresión termica">
                            <f:fields>
                                    <RadioButton groupName="GroupA" id="opcion3"  />
                            </f:fields>
                        </f:FormElement>

                    </f:formElements>
                </f:FormContainer>
            </f:formContainers>
        </f:Form>
        

        </content>
        <buttons>
            <Button text="Generar PDF" type="Accion" press="onOpenPreviewPDF" icon="sap-icon://pdf-reader"/>
            <Button text="Imprimir" type="Accion" icon="sap-icon://print" press="onImprimirDoc"/>
            <Button text="Cancel" type="Reject" icon="sap-icon://sys-cancel" press="onCloseDocImprimir"/>
        </buttons>
    </Dialog>


