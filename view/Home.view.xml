    <Dialog title="Impresión de Reclamo" id="dlg_rec_impresion" class="cabecera_dialogo" stretchOnPhone="true" contentWidth="auto" contentHeight="auto">
        <content>
            <Page>
                <customHeader>
                    <Toolbar>
                        <Label text="Número de Reclamo:"></Label>
                        <SearchField width="55%" placeholder="Ingrese Nro. de Reclamo" value="{/imprimirRec/pNumeroReclamo}" showSearchButton="false"></SearchField>
                    </Toolbar>
                </customHeader>
            </Page>
        </content>
        <beginButton>
            <Button text="Imprimir" type="Accion" icon="sap-icon://print" press="onImprimirRec" />
        </beginButton>
        <endButton>
            <Button text="Cancel" type="Reject" icon="sap-icon://sys-cancel" press="onCloseRecImprimir" />
        </endButton>
    </Dialog>


