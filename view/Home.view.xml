                        <Page id="pgCorreo" title="Editor de correo">
                            <content>
                                    <f:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanL="12" labelSpanM="12" emptySpanL="1" emptySpanM="0" columnsL="0" columnsM="0">
