<core:FragmentDefinition 
    height="100%" 
    xmlns:core="sap.ui.core" 
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns="sap.m"
    xmlns:l="sap.ui.layout" 
    xmlns:f="sap.ui.layout.form"
    xmlns:t="sap.ui.table" 
    xmlns:s="sap.m.semantic" 
    xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    displayBlock="true">
        
<BusyDialog id="loadingControl4" />
 
<s:DetailPage title="{/nombre} {/recNuevo/pNumeroReclamo}" id="pag_rec_nuevo_reclamo" class="cabecera_semanticDetail">
                <s:content>
                    <IconTabBar stretchContentHeight="false" expandable="false" headerMode="Inline" upperCase="false" expanded="{device&gt;/isNoPhone}" class="sapUiResponsiveContentPadding">
                        <items>
                            <IconTabFilter showAll="false" text="Materiales" icon="sap-icon://person-placeholder" design="Vertical">


                                <f:Form editable="true">
                            <f:layout>
                                <f:ResponsiveGridLayout
                                    labelSpanXL="3"
                                    labelSpanL="3"
                                    labelSpanM="3"
                                    labelSpanS="6"
                                    adjustLabelSpan="false"
                                    emptySpanXL="4"
                                    emptySpanL="4"
                                    emptySpanM="4"
                                    emptySpanS="0"
                                    columnsXL="1"
                                    columnsL="1"
                                    columnsM="1"
                                    singleContainerFullSize="false" />
                            </f:layout>
                            <f:formContainers>
                                <f:FormContainer>
                                    <f:formElements>
                                         <f:FormElement label="Material 1">
                                                <f:fields>
                                                    <ComboBox width="100%" items="{ path : '/listaMateriales' }" 
                                                        selectionChange="onSeleccionarMatRec" id="cbo_material21"  selectedKey="{/recNuevo/material1}" enabled="{/enabled}">
                                                        <core:Item key="{CodMaterialCorto}" text="{CodMaterialCorto} - {DescMaterial}" />
                                                    </ComboBox><!--required="true"-->
                                                </f:fields>
                                            </f:FormElement>
                                            <f:FormElement label="Material 2" visible="false">
                                                <f:fields>
                                                    <ComboBox width="100%" items="{ path : '/listaMateriales' }" 
                                                        change="onSeleccionarMatRec" id="cbo_material22" selectedKey="{/recNuevo/material2}" enabled="{/enabled}">
                                                        <core:Item key="{CodMaterialCorto}" text="{CodMaterialCorto} - {DescMaterial}" />
                                                    </ComboBox>
                                                </f:fields>
                                            </f:FormElement>
                                            <f:FormElement label="Cantidad Material 1">
                                                <f:fields>
                                                    <Input placeholder="Ingrese cantidad Material 1" width="100%" id="txt_cantRecla1" value="{/recNuevo/cantRecla1}" enabled="{/enabled}" showSearchButton="false" type="Number" pattern="[0-9]*" change="onInputCantMatRec"></Input>
                                                </f:fields>
                                            </f:FormElement>
                                            <f:FormElement label="Cantidad Material 2" visible="false">
                                                <f:fields>
                                                    <Input placeholder="Ingrese cantidad Material 2" width="100%" id="txt_cantRecla2" value="{/recNuevo/cantRecla2}" enabled="{/enabled}" showSearchButton="false" type="Number" pattern="[0-9]*" change="onInputCantMatRec"></Input>
                                                </f:fields>
                                            </f:FormElement>
                                            <f:FormElement ><!--label="reclamo Referencia"-->
                                                <f:fields>
                                                    <SearchField placeholder="Ingrese Referencia" width="100%" id="txt_reclamoRef" value="{/recNuevo/reclamoRef}" enabled="{/enabled}" showSearchButton="false" visible="false"></SearchField>
                                                </f:fields>
                                            </f:FormElement>
                                        
                                    </f:formElements>
                                </f:FormContainer>
                            </f:formContainers>
                        </f:Form>


                                
                            </IconTabFilter>
                            <IconTabFilter showAll="false" text="Cliente" icon="sap-icon://add-contact" design="Vertical">

                                <f:Form editable="true">
                            <f:layout>
                                <f:ResponsiveGridLayout
                                    labelSpanXL="3"
                                    labelSpanL="3"
                                    labelSpanM="3"
                                    labelSpanS="6"
                                    adjustLabelSpan="false"
                                    emptySpanXL="4"
                                    emptySpanL="4"
                                    emptySpanM="4"
                                    emptySpanS="0"
                                    columnsXL="1"
                                    columnsL="1"
                                    columnsM="1"
                                    singleContainerFullSize="false" />
                            </f:layout>
                            <f:formContainers>
                                <f:FormContainer>
                                    <f:formElements>
                                        
                                         <f:FormElement label="Nro. de Pedido">
                                            <f:fields>
                                                <Input width="100%" id="txt_Numero_Pedido" value="{/recNuevo/numeroPedido}" enabled="{/enabled}" editable="false"></Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Cliente">
                                            <f:fields>
                                               <Input width="100%" id="txt_Codigo_Cliente" value="{/recNuevo/codigoEmpResp}" enabled="{/enabled}" editable="false"></Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="CLIENTE EVENTUAL">
                                            <f:fields>
                                                <Input width="100%" id="txt_cliente_eventual" value="{/recNuevo/NomCliente}" enabled="{/enabled}" editable="false"></Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Asesor de Pedido de Venta">
                                            <f:fields>
                                                <ComboBox width="100%" items="{ path : 'modellstAsesores>/' }" 
                                                        change="onSeleccionaSurtido" required="true" selectedKey="{/recNuevo/codAsesorPedVenta}" id="cbo_asesor" editable="false">
                                                    <core:Item key="{modellstAsesores>Codigo}"  text="{modellstAsesores>Descripcion}" />
                                                </ComboBox>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Asesor que generÃ³ Reclamo">
                                            <f:fields>
                                                <Input width="100%" 
                                                    value="{/recNuevo/asesorGenRec}" editable="false">        
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Asesor que genero Reclamo" visible="false">
                                            <f:fields>
                                                <Input width="100%" id="txt_nombre_codigo_asesor" 
                                                    value="{/objReclamo/Interlocutor/ZM/NOMBRE}" editable="false">        
                                                </Input>
                                            </f:fields>
                                        </f:FormElement>
                                    </f:formElements>
                                </f:FormContainer>
                            </f:formContainers>
                        </f:Form>


                                                <ComboBox width="100%" items="{ path : '/dataIni/lstMotivoRecl' }" change="onSeleccionaSurtido" required="true" id="cbo_Motivo" selectedKey="{/recNuevo/Motivo}" enabled="{/enabled}" visible="false">
                                                    <core:Item key="{Codigo}" text="{Descripcion}" />
                                                </ComboBox>
                                                <ComboBox width="100%" items="{ path : '/dataIni/lstStatusRecl' }" change="onSeleccionaSurtido" required="true" id="cbo_status" selectedKey="{/recNuevo/Status}" enabled="{/enabled}" visible="false">
                                                    <core:Item key="{Codigo}" text="{Descripcion}" />
                                                </ComboBox>
                                                <ComboBox width="100%" items="{ path : '/dataIni/lstResultRecl' }" change="onSeleccionaSurtido" required="true" id="cbo_Resultado" selectedKey="{/recNuevo/Resultado}" enabled="{/enabled}" visible="false">
                                                    <core:Item key="{Codigo}" text="{Descripcion}" />
                                                </ComboBox>
                                                <ComboBox width="100%" items="{ path : '/dataIni/lstJustiResul' }" change="onSeleccionaSurtido" required="true" id="cbo_JustificResul" selectedKey="{/recNuevo/JustificResul}" enabled="{/enabled}" visible="false">
                                                    <core:Item key="{Codigo}" text="{Descripcion}" />
                                                </ComboBox>



                                                <ComboBox width="100%"  change="onSeleccionaSurtido" required="true" id="cbo_OrgVenta" selectedKey="{/recNuevo/OrgVenta}" enabled="{/enabled}" visible="false">
                                                    <core:Item key="{/dataIni/person/OrgVentas}" text="{/dataIni/person/OrgVentas} :: DECOR CENTER" />
                                                </ComboBox>
                                                <ComboBox width="100%" items="{ path : '/dataIni/lstCanales' }" change="onSeleccionaSurtido" required="true" id="cbo_Canal" selectedKey="{/recNuevo/Canal}"  enabled="{/enabled}" visible="false">
                                                   <core:Item key="{Codigo}" text="{Descripcion}" />
                                               </ComboBox>
                                                <ComboBox width="100%" items="{ path : '/dataIni/lstSectores' }" change="onSeleccionaSurtido" required="true" id="cbo_Sector" selectedKey="{/recNuevo/Sysnr}" enabled="{/enabled}" visible="false">
                                                    <core:Item key="{Codigo}" text="{Descripcion}" />
                                                </ComboBox>
                                                <ComboBox width="100%" items="{ path : '/dataIni/lstOfVtas' }" change="onSeleccionaSurtido" required="true" id="cbo_OfiVenta" 
                                                    selectedKey="{/recNuevo/OfiVenta}" enabled="{/enabled}" visible="false">
                                                    <core:Item key="{Codigo}" text="{Descripcion}" />
                                                </ComboBox>
                                                <SearchField width="100%" id="txt_factura" value="{/recNuevo/comentario}" placeholder="Ingrese Factura / Boleta" enabled="{/enabled}" showSearchButton="false" visible="false"></SearchField>
                               
                            </IconTabFilter>
                        </items>
                    </IconTabBar>
                </s:content>
                <s:pagingAction>
                    <Button icon="sap-icon://save" id="btn_guardar1" tooltip="Guardar" press="onGuardarReclamo"/>
                </s:pagingAction>
                <!--s:customFooterContent>
                    <Button text="Buscar" icon="sap-icon://search" press="onOpenDlgBuscarRecNuevo"></Button>
                </s:customFooterContent-->
            </s:DetailPage>
        
    </core:FragmentDefinition>