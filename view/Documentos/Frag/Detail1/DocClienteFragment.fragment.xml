<core:FragmentDefinition 
    xmlns:core="sap.ui.core" 
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns="sap.m"
     xmlns:l="sap.ui.layout" 
     xmlns:f="sap.ui.layout.form"
    xmlns:t="sap.ui.table" 
    xmlns:s="sap.m.semantic" 
    xmlns:smartForm="sap.ui.comp.smartform" 
    xmlns:smartField="sap.ui.comp.smartfield"
    xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    <BusyDialog id="loadingControl" />
    <!--Aviso General-->
    <core:Fragment fragmentName="pe.com.seidor.sap.decor.ventas.view.Documentos.FragDialogs.dlg_MensajeAvisoGeneral" type="XML"></core:Fragment>

    <!--dlg_MensajeAvisoCrearCotizacion-->
    <core:Fragment fragmentName="pe.com.seidor.sap.decor.ventas.view.Documentos.FragDialogs.dlg_MensajeAvisoCrearCotizacion" type="XML"></core:Fragment>

    <!--dlg_MensajeAvisoPedidoCreado-->
    <core:Fragment fragmentName="pe.com.seidor.sap.decor.ventas.view.Documentos.FragDialogs.dlg_MensajeAvisoPedidoCreado" type="XML"></core:Fragment>


    <!--dlg_DialogDocNuevo-->
            <core:Fragment fragmentName="pe.com.seidor.sap.decor.ventas.view.Documentos.FragDialogs.dlg_DialogDocNuevoInicio" type="XML"></core:Fragment>

    <!--dlg_DialogDocModificar-->
            <core:Fragment fragmentName="pe.com.seidor.sap.decor.ventas.view.Documentos.FragDialogs.dlg_DialogDocModificarInicio" type="XML"></core:Fragment>

    <!--dlg_DialogDocVisualizar-->
            <core:Fragment fragmentName="pe.com.seidor.sap.decor.ventas.view.Documentos.FragDialogs.dlg_DialogDocVisualizarInicio" type="XML"></core:Fragment>
    

    <s:DetailPage title="{/tituloDocumento}" id="pagDocNuevo_datos_detail1" class="cabecera_semanticDetail">
        
        <s:content>
            
            <ObjectHeader id="txt_nombreCliente" backgroundDesign="Solid" title="{/clienteEventual/nombreCliente}">
                <attributes>
                    <ObjectAttribute id="txt_codigoCliente" title="Código" text="{/clienteEventual/codigoCliente}"/>
                </attributes>
            </ObjectHeader>
            <!--ObjectHeader
                icon="sap-icon://save"
                iconActive="true"
                iconRight="true"
        responsive="true"
        binding="{/ProductCollection/0}"
        intro="{Description}"
        title="{Name}"
        number="{Price}"
        numberUnit="{CurrencyCode}"
        showMarkers="true"
        markFlagged="true"
        markFavorite="true"
        numberState="Success"
        backgroundDesign="Translucent">

        <attributes>
            <ObjectAttribute
                title="Manufacturer"
                text="{SupplierName}"
                img="sap-icon://save"/>
        </attributes>
        <statuses>
            
        </statuses>

        <headerContainer>
            
            <HeaderContainer scrollStep="200" scrollTime="500" >
                <FlexBox
                    alignItems="Center"
                    justifyContent="Center">
                    <items>
                
            </items>
        </FlexBox>
               
            </HeaderContainer>

        </headerContainer>
             </ObjectHeader-->


            <IconTabBar expandable="false" headerMode="Inline" upperCase="false" expanded="{device&gt;/isNoPhone}" class="sapUiResponsiveContentPadding contenedorTabBar" >
                
                <items>
                    <IconTabFilter showAll="false" text="Área de Ventas" icon="sap-icon://my-sales-order" design="Vertical">                                   
                               
                <FlexBox
                    alignItems="Center"
                    justifyContent="Center">
                    <items>
                        <f:Form id="form_OrgVentas" editable="true">
                            <f:layout>
                                <f:ResponsiveGridLayout
                                    labelSpanXL="12"
                                    labelSpanL="12"
                                    labelSpanM="12"
                                    labelSpanS="12"
                                    adjustLabelSpan="false"
                                    emptySpanXL="0"
                                    emptySpanL="0"
                                    emptySpanM="0"
                                    emptySpanS="0"
                                    columnsXL="1"
                                    columnsL="1"
                                    columnsM="1"
                                    singleContainerFullSize="false" />
                            </f:layout>
                            <f:formContainers>
                                <f:FormContainer>
                                    <f:formElements>
                                        <f:FormElement label="Organización de Ventas">
                                            <f:fields>
                                                <Select  maxwidth="100%" required="true" id="com_orgVentas_areaVentas" enabled="{/pedido/enabled}">
                                                    <core:Item key="" text="{/dataIni/person/OrgVentas} :: DECOR CENTER"/>
                                                </Select>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Canal">
                                            <f:fields>
                                                 <Select items="{ path : '/dataIni/lstCanales' }" maxwidth="100%" selectedKey="{/pedido/CanalDist}" required="true" id="com_Canal_areaVentas" enabled="{/pedido/enabled}">
                                                    <core:Item key="{Codigo}" text="{Descripcion}"/>
                                                </Select>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Oficina">
                                            <f:fields>
                                                <Select items="{ path : '/dataIni/lstOfVtas' }" selectedKey="{/pedido/CodOficina}" maxwidth="100%" required="true" id="com_oficina_areaVentas" enabled="{/pedido/enabled}">
                                                    <core:Item key="{Codigo}" text="{Descripcion}"/>
                                                </Select>
                                            </f:fields>
                                        </f:FormElement>
                                    </f:formElements>
                                </f:FormContainer>
                            </f:formContainers>
                        </f:Form>
                    </items>
                </FlexBox>                                    
                    </IconTabFilter>
                    <IconTabFilter showAll="false" text="Pago " icon="sap-icon://simple-payment" design="Vertical">

                        <FlexBox
                    alignItems="Center"
                    justifyContent="Center">
                        <f:Form editable="true">
                            <f:layout>
                                <f:ResponsiveGridLayout
                                     labelSpanXL="12"
                                    labelSpanL="12"
                                    labelSpanM="12"
                                    labelSpanS="12"
                                    adjustLabelSpan="false"
                                    emptySpanXL="0"
                                    emptySpanL="0"
                                    emptySpanM="0"
                                    emptySpanS="0"
                                    columnsXL="1"
                                    columnsL="1"
                                    columnsM="1"
                                    singleContainerFullSize="false" />
                            </f:layout>
                            <f:formContainers>
                                <f:FormContainer>
                                    <f:formElements>
                                        <f:FormElement label="Condición de Pago">
                                            <f:fields>
                                                <Select items="{ path : '/dataIni/lstCPag' }" maxwidth="100%" required="true" selectedKey="{/pedido/CondPago}" id="com_condPago_pago" enabled="{/pedido/enabled}">
                                    <core:Item key="{Codigo}" text="{Descripcion}"/>
                                </Select>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Moneda">
                                            <f:fields>
                                                 <Select items="{ path : '/dataIni/lstMoneda' }" maxwidth="100%" required="true" selectedKey="{/pedido/Moneda}" id="com_moneda_pago" enabled="{/pedido/enabled}" change="cambiar_moneda">
                                    <core:Item key="{Codigo}" text="{Descripcion}"/>
                                </Select>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Tipo de Cambio">
                                            <f:fields>
                                                <Input width="100%" placeholder="Ingrese Tipo de Cambio" value="{
                                                            path: '/pedido/TipoCambio',
                                                            type: 'sap.ui.model.type.Float',
                                                            formatOptions: {
                                                                 decimalSeparator : '.',
                                                                minFractionDigits: 3
                                                            } }" id="txt_tipoCambio_pago" enabled="{/pedido/enabled}" editable="false"></Input>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Peso Total">
                                            <f:fields>
                                                <Input width="100%" placeholder="Ingrese Peso Total" value="0.000 KG" id="txt_pago_pesoTotal" enabled="{/pedido/enabled}"></Input>
                                            </f:fields>
                                        </f:FormElement>
                                    </f:formElements>
                                </f:FormContainer>
                            </f:formContainers>
                        </f:Form>
                    </FlexBox>
                    </IconTabFilter>
                    <IconTabFilter showAll="false" text="Datos Facturac." icon="sap-icon://sales-order-item" design="Vertical">
                        <f:Form editable="true">
                            <f:layout>
                                <f:ResponsiveGridLayout
                                     labelSpanXL="12"
                                    labelSpanL="12"
                                    labelSpanM="12"
                                    labelSpanS="12"
                                    adjustLabelSpan="false"
                                    emptySpanXL="0"
                                    emptySpanL="0"
                                    emptySpanM="0"
                                    emptySpanS="0"
                                    columnsXL="2"
                                    columnsL="2"
                                    columnsM="2"
                                    singleContainerFullSize="false" 

                                    />
                            </f:layout>
                            <f:formContainers>
                                <f:FormContainer>
                                    <f:formElements>
                                        <f:FormElement label="Fecha Facturación">
                                            <f:fields>
                                                <DatePicker  valueFormat="dd/MM/YYYY" displayFormat="dd/MM/YYYY"
                                    value="{/pedido/FechaFacturacion}" id="date_fechaFacturacion_datosFacturacion" enabled="{/pedido/enabled}">
                                    <layoutData>
                                        <l:GridData span="L12 M12 S12"/>
                                    </layoutData>
                                </DatePicker>
                                            </f:fields>
                                        </f:FormElement>
                                    </f:formElements>
                                </f:FormContainer>

                            </f:formContainers>
                        </f:Form>


                        <f:Form editable="true">
                            <f:layout>
                                <f:ResponsiveGridLayout
                                    labelSpanXL="12"
                                    labelSpanL="12"
                                    labelSpanM="12"
                                    labelSpanS="12"
                                    adjustLabelSpan="false"
                                    emptySpanXL="0"
                                    emptySpanL="0"
                                    emptySpanM="0"
                                    emptySpanS="0"
                                    columnsXL="2"
                                    columnsL="2"
                                    columnsM="2"
                                    singleContainerFullSize="false" />
                            </f:layout>
                            <f:formContainers>
                                <f:FormContainer>
                                    <f:formElements>
                                        
                                        <f:FormElement label="Nombre Banco">
                                            <f:fields>
                                                 <Select items="{ path : '/dataIni/lstGrpCond' }" maxwidth="100%" required="true" selectedKey="{/pedido/NombreBanco}" id="com_nombreBanco_datosFacturacion" enabled="{/pedido/enabled}">
                                    <core:Item key="{Codigo}" text="{= ${Codigo} === ''? 'Seleccione Banco' : ${Descripcion} }"/>
                                </Select>
                                            </f:fields>
                                        </f:FormElement>

                                        <f:FormElement label="Motivo de NC o ND">
                                            <f:fields>
                                                <Select items="{ path : '/dataIni/lstMotPed' }" maxwidth="100%" required="true"  selectedKey="{/pedido/Motivo}" id="com_motivoNcNd_datosFacturacion" enabled="{/pedido/enabled}">
                                    <core:Item key="{Codigo}" text="{= ${Codigo} === ''? 'Seleccione Motivo' : ${Descripcion} }"/>
                                </Select>
                                            </f:fields>
                                        </f:FormElement>                                                                            
                                    </f:formElements>                                                                                                        
                                </f:FormContainer>
                                        
                                <f:FormContainer>
                                    <f:formElements>
                                        <f:FormElement label="Bloqueo Factura">
                                            <f:fields>
                                                <Select items="{ path : '/dataIni/lstBloFac' }" maxwidth="100%" required="true" selectedKey="{/pedido/BloqueoFactura}" id="com_bloqueoFactura_datosFacturacion" enabled="{/pedido/enabled}">
                                    <core:Item key="{Codigo}" text="{= ${Codigo} === ''? 'Seleccione' : ${Descripcion} }"/>
                                </Select>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Bloqueo Entrega">
                                            <f:fields>
                                                <Select items="{ path : '/dataIni/lstBloEnt' }" maxwidth="100%" required="true" selectedKey="{/pedido/BloqueoEntrega}" id="com_bloqueoEntrega_datosFacturacion" enabled="{/pedido/enabled}">
                                    <core:Item key="{Codigo}" text="{= ${Codigo} === ''? 'Seleccione' : ${Descripcion} }"/>
                                </Select>
                                            </f:fields>
                                        </f:FormElement>
                                        </f:formElements>

                                </f:FormContainer>

                            </f:formContainers>
                        </f:Form>
                        
                    </IconTabFilter>
                    <IconTabFilter showAll="false" text="Datos Documento" icon="sap-icon://receipt" design="Vertical">

                        <f:Form editable="true">
                            <f:layout>
                                <f:ResponsiveGridLayout
                                    labelSpanXL="12"
                                    labelSpanL="12"
                                    labelSpanM="12"
                                    labelSpanS="12"
                                    adjustLabelSpan="false"
                                    emptySpanXL="0"
                                    emptySpanL="0"
                                    emptySpanM="0"
                                    emptySpanS="0"
                                    columnsXL="2"
                                    columnsL="2"
                                    columnsM="2"
                                    singleContainerFullSize="false" />
                            </f:layout>
                            <f:formContainers>
                                <f:FormContainer>
                                    <f:formElements>
                                        <f:FormElement label="Nro. Orden de Compra">
                                            <f:fields>
                                                <SearchField
                                                    width="100%" placeholder="Ingrese Nro. de Orden de Compra" value="{/pedido/OrdenCompra}" id="txt_nroOrdenCompra_datosDocumento" enabled="{/pedido/enabled}"
                                                    showSearchButton="false"
                                                    >
                                                     <layoutData>
                                                        <l:GridData span="L12 M12 S12"/>
                                                    </layoutData>
                                                </SearchField>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Fecha Pedido">
                                            <f:fields>
                                                 <DatePicker  valueFormat="dd/MM/YYYY" displayFormat="dd/MM/YYYY"
                                    value="{/pedido/FechaPedido}" id="date_fechaPedido_datosDocumento" enabled="{/pedido/enabled}">
                                    <layoutData>
                                        <l:GridData span="L12 M12 S12"/>
                                    </layoutData>
                                </DatePicker>
                                            </f:fields>
                                        </f:FormElement>
                                                                                
                                    </f:formElements>
                                </f:FormContainer>


                            <f:FormContainer>
                                    <f:formElements>
                                        <f:FormElement label="Fecha de Validez">
                                            <f:fields>
                                                 <DatePicker valueFormat="dd/MM/YYYY" displayFormat="dd/MM/YYYY"
                                    value="{/pedido/FechaValidez}" id="date_fechaValidez_datosDocumento" enabled="{/pedido/enabled}">
                                   
                                </DatePicker>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Fecha Ent. Referencial">
                                            <f:fields>
                                                 <DatePicker  valueFormat="dd/MM/YYYY" displayFormat="dd/MM/YYYY"
                                    value="{/pedido/FechaEntrega}" id="date_fechaEntReferencial_datosDocumento" enabled="{/pedido/enabled}">
                                    <layoutData>
                                        <l:GridData span="L12 M12 S12"/>
                                    </layoutData>
                                </DatePicker>
                                            </f:fields>
                                        </f:FormElement>
                                            <f:FormElement label="Tipo de Despacho">
                                            <f:fields>
                                                <Select items="{ path : '/dataIni/lstCondExp' }" maxwidth="100%" required="true"  selectedKey="{/pedido/CondExp}" id="com_tipoDespacho_datosDocumento" enabled="{/pedido/enabled}">
                                    <core:Item key="{Codigo}" text="{Descripcion}"/>
                                </Select>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Fecha de Despacho - Reparto">
                                            <f:fields>
                                                <DatePicker valueFormat="dd/MM/YYYY" displayFormat="dd/MM/YYYY"
                                    value="{parts: [{path: 'date>/'}, {path: 'formatdate>/short'}, {path: 'formatdate>/equal'}], formatter: '.formatter.date'}" id="date_fechaDespachoReparto_datosDocumento" enabled="{/pedido/enabled}">
                                    
                                </DatePicker>
                                            </f:fields>
                                        </f:FormElement>
                                    </f:formElements>
                                </f:FormContainer>
                            </f:formContainers>
                        </f:Form>
                        
                    </IconTabFilter>
                    <IconTabFilter showAll="false" text="Proyecto y Visita" icon="sap-icon://visits" design="Vertical">


                    <f:Form editable="true">
                            <f:layout>
                                <f:ResponsiveGridLayout
                                    labelSpanXL="12"
                                    labelSpanL="12"
                                    labelSpanM="12"
                                    labelSpanS="12"
                                    adjustLabelSpan="false"
                                    emptySpanXL="4"
                                    emptySpanL="4"
                                    emptySpanM="4"
                                    emptySpanS="0"
                                    columnsXL="2"
                                    columnsL="2"
                                    columnsM="2"
                                    singleContainerFullSize="false" />
                            </f:layout>
                            <f:formContainers>
                                <f:FormContainer>
                                    <f:formElements>
                                        <f:FormElement label="Nombre Proyecto">
                                            <f:fields>
                                                <SearchField
                                                    placeholder="Ingrese Proyecto" value="{/pedido/nomProyecto}"  id="txt_nombreProyecto_proyectoVisita" enabled="{/pedido/enabled}"
                                                    showSearchButton="false"
                                                    >
                                                     <layoutData>
                                                        <l:GridData span="L12 M12 S12"/>
                                                    </layoutData>
                                                </SearchField>
                                            </f:fields>
                                        </f:FormElement>                                                                                
                                        
                                    </f:formElements>
                                </f:FormContainer>
                                <f:FormContainer>
                                    <f:formElements>
                                        <f:FormElement label="Tipo de Visita">
                                            <f:fields>
                                                 <Select items="{ path : '/dataIni/lstTipoVisita' }" required="true" selectedKey="{/pedido/TipoVisita}" id="com_tipoVisita_proyectoVisita" enabled="{/pedido/enabled}">
                                    <core:Item key="{Codigo}" text="{= ${Codigo} === ''? 'Seleccione Tipo de Visita' : ${Descripcion} }"/>
                                    <layoutData>
                                        <l:GridData span="L12 M12 S12"/>
                                    </layoutData>
                                </Select>
                                            </f:fields>
                                        </f:FormElement>
                                    </f:formElements>
                                </f:FormContainer>
                            </f:formContainers>
                        </f:Form>
                        <f:Form editable="true">
                            <f:layout>
                                <f:ResponsiveGridLayout
                                    labelSpanXL="12"
                                    labelSpanL="12"
                                    labelSpanM="12"
                                    labelSpanS="12"
                                    adjustLabelSpan="false"
                                    emptySpanXL="4"
                                    emptySpanL="4"
                                    emptySpanM="4"
                                    emptySpanS="0"
                                    columnsXL="2"
                                    columnsL="2"
                                    columnsM="2"
                                    singleContainerFullSize="false" />
                            </f:layout>
                            <f:formContainers>
                                <f:FormContainer>
                                    <f:formElements>
                                        <f:FormElement label="Código Proyecto">
                                            <f:fields>
                                                <SearchField
                                                    placeholder="Código Proyecto" value="{/pedido/codProyecto}"  id="txt_codigoProyecto_proyectoVisita" enabled="{/pedido/enabled}"
                                                    showSearchButton="false"
                                                    >
                                                     <layoutData>
                                                        <l:GridData span="L12 M12 S12"/>
                                                    </layoutData>
                                                </SearchField>
                                            </f:fields>
                                        </f:FormElement>
                                        <f:FormElement label="Código Version">
                                            <f:fields>
                                                <SearchField
                                                    placeholder="Código Versión" value="{/pedido/codVersion}"  id="txt_codigoVersion_proyectoVisita" enabled="{/pedido/enabled}"
                                                    showSearchButton="false"
                                                    >
                                                     <layoutData>
                                                        <l:GridData span="L12 M12 S12"/>
                                                    </layoutData>
                                                </SearchField>
                                            </f:fields>
                                        </f:FormElement>
                                        
                                         <f:FormElement label="Visita No Reembolsable">
                                            <f:fields>
                                                <CheckBox selected="{/pedido/Reenbolsable}" id="check_visitaNoReembolsable_proyectoVisita" enabled="{/pedido/enabled}">
                                                    <layoutData>
                                        <l:GridData span="L12 M12 S12"/>
                                    </layoutData>
                                                </CheckBox>
                                            </f:fields>
                                        </f:FormElement>                                    
                                    </f:formElements>
                                </f:FormContainer>

                                <f:FormContainer>
                                    <f:formElements>
                                            
                                       <f:FormElement label="Grupo Forecast">
                                            <f:fields>
                                                 <Select items="{ path : '/lstGrupoFor' }"  required="true" selectedKey="{/pedido/GrupoForecast}" id="com_grupoForecast_proyectoVisita" enabled="{/pedido/enabled}">
                                    <core:Item key="{Codigo}" text="{Descripcion}"/>
                                    <layoutData>
                                        <l:GridData span="L12 M12 S12"/>
                                    </layoutData>
                                </Select>
                                            </f:fields>
                                        </f:FormElement>

                                        <f:FormElement label="Tipo Forecast">
                                            <f:fields>
                                                 <Select items="{ path : '/lstTipoFor' }"  required="true" selectedKey="{/pedido/TipoForecast}" id="com_tipoForecast_proyectoVisita" enabled="{/pedido/enabled}">
                                    <core:Item key="{Codigo}" text="{= ${Codigo} === ' '? 'Seleccione Tipo Forecast' : ${Descripcion} }"/>
                                    <layoutData>
                                        <l:GridData span="L12 M12 S12"/>
                                    </layoutData>
                                </Select>
                                            </f:fields>
                                        </f:FormElement>

                                        <f:FormElement label="Nro. Certificado">
                                            <f:fields>
                                                <SearchField
                                                    placeholder="Ingrese Nro. Certificado" value="{/pedido/Certificado}" id="txt_nroCertificado_proyectoVisita" enabled="{/pedido/enabled}"
                                                    showSearchButton="false"
                                                    >
                                                     <layoutData>
                                                        <l:GridData span="L12 M12 S12"/>
                                                    </layoutData>
                                                </SearchField>
                                            </f:fields>
                                        </f:FormElement>

                                        <f:FormElement label="Fecha de Visita">
                                            <f:fields>
                                                  <DatePicker  valueFormat="dd/MM/YYYY" displayFormat="dd/MM/YYYY"
                                    value="{/pedido/FechaVisita}" id="date_fechaVisita_proyectoVisita" enabled="{/pedido/enabled}">
                                    <layoutData>
                                        <l:GridData span="L12 M12 S12"/>
                                    </layoutData>
                                </DatePicker>
                                            </f:fields>
                                        </f:FormElement>                                                                            
                                    </f:formElements>
                                </f:FormContainer>
                            </f:formContainers>
                        </f:Form>

                    </IconTabFilter>
                </items>

            </IconTabBar>
            
        </s:content>

                
        <s:pagingAction>
            <Button icon="sap-icon://save"  tooltip="Guardar" press="onBtnGuardarDocumento" visible="{/pedido/enabledBtnGuardar}"/>
        </s:pagingAction>




        <!--s:printAction>
            <s:PrintAction press="onDocNuevoSemanticButtonPress"/>
        </s:printAction-->
        <s:customFooterContent>
                    <Button type="Transparent" class="btnCarrito" icon="sap-icon://cart" press="onDocNuevoMasterProductos" font-size="60px"/>
        </s:customFooterContent>
    </s:DetailPage>
</core:FragmentDefinition>