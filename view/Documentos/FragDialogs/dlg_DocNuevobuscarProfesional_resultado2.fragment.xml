<core:FragmentDefinition 
    xmlns:core="sap.ui.core" 
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns="sap.m"
     xmlns:l="sap.ui.layout" 
     xmlns:f="sap.ui.layout.form"
    xmlns:t="sap.ui.table" 
    xmlns:s="sap.m.semantic" 
    xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    	
    <Dialog title="Seleccionar Profesional" id="dlg_DocNuevobuscarProfesional_resultado2" contentWidth="auto" contentHeight="auto">
                <content>
                    <List id="resultadoListaProfesional2" selectionChange="seleccionaProfesional2" mode="SingleSelectMaster" growing="false" growingThreshold="30" growingDirection="Upwards" busyIndicatorDelay="200"
                        enableBusyIndicator="false" growingTriggerText="Previous Products" noDataText="No hay Profesionales" itemPress=""  items="{
                                        path : '/BusquedaProfesionales',
                                        sorter : {
                                            path : 'Descripcion',
                                            group: false
                                        }
                                    }">

                                    <CustomListItem height="auto">
                <FlexBox
                        alignItems="Start"
                        justifyContent="SpaceBetween" >
                        <items>
                                <FlexBox
                        alignItems="Start" width="100%">
                        <items>

                        <VBox justifyContent="Center" width="20px">
                                </VBox>
                                <FlexBox
                                    height="45px"
                        alignItems="Start"
                        justifyContent="Start">
                        <items>

                                <Button width="100%" height="auto"  type="Transparent" icon="sap-icon://customer" press="" >
                                    <layoutData>
                                <FlexItemData growFactor="1" />
                            </layoutData>
                                </Button>

                            </items>
                        </FlexBox>
                        <VBox justifyContent="Center" width="35px">
                                </VBox>
                        <FlexBox
                                    height="35px"
                        alignItems="End"
                        justifyContent="End">
                        <items>

                                <Text width="100%" class="lblDetalleProducto" text="{codigo} \n {nombrecliente}">
                                    <layoutData>
                                <FlexItemData growFactor="3" />
                            </layoutData>
                                </Text>

                            </items>
                        </FlexBox>
                        
                              </items>
                            </FlexBox>

                                

                    </items>
                </FlexBox>
                </CustomListItem>
                       
                    </List>
                </content>
        <endButton>
            <Button text="Cancel" type="Reject" icon="sap-icon://sys-cancel" press="onCancelDlg_DocNuevobuscarProfesional_resultado2"/>
        </endButton>
    </Dialog>
    </core:FragmentDefinition>