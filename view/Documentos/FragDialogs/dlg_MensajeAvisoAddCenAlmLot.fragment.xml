        <content>
            <Text id="lb_datos" text="" visible="false"></Text>
            <Text text="Desea agregar el Centro/Lote/Almacen?"></Text>
        </content>
        <beginButton>
            <Button text="Si" type="Accept" press="onSiMensajeAvisoAddCenAlmLot"></Button>
        </beginButton>
        <endButton>
            <Button text="No" type="Reject" press="onNoMensajeAvisoAddCenAlmLot"></Button>
        </endButton>
    </Dialog>



