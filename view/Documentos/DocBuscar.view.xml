                              itemPress="onListaDetalleDoc"  
                              items="{
                        path : '/datosBuscarDoc/Detalle'
                        }"
                >

                <CustomListItem  width="auto">
                    <l:VerticalLayout
                    class="equalColumns"
                    width="100%" >
                    <l:content>
                        <FlexBox
                         width="100%"
                         class="columns"
                         alignItems="Stretch">
                                <items>
                                    <Button class="column1" type="Transparent" icon="sap-icon://notes" press="" >
                                        <layoutData>
                                            <FlexItemData growFactor="1" />
                                        </layoutData>
                                    </Button>
                                    <Text class="lblDetalleProducto column2" text="{Posicion} - {CodigoMaterial} \n {Material}">
                                        <layoutData>
                                            <FlexItemData growFactor="5" />
                                        </layoutData>
                                    </Text>
                                    <Text width="100%" class="lblDetalleProducto column2" text="Cantidad: {Cantidad}" textAlign="Center">
                                        <layoutData>
                                            <FlexItemData growFactor="3" />
                                        </layoutData>
                                    </Text>
                                    <Text width="100%" class="lblDetalleProducto column2" text="Centro Almacen: {CentroAlmacen} \n Lote: {Lote} " textAlign="Center">
                                        <layoutData>
                                            <FlexItemData growFactor="3" />
                                        </layoutData>
                                    </Text>
                                    <Text width="100%" class="lblDetalleProducto column2" text="Fecha Reparto: {FechaReparto} \n Cantidad Conf. : {CantidadConf} " textAlign="Center">
                                        <layoutData>
                                            <FlexItemData growFactor="3" />
                                        </layoutData>
                                    </Text>
                                    <Text width="100%" class="lblDetalleProducto column2" text="Fecha Conf.: {FConf} \n Motivo Rechazo: {MotivoRechazo}" textAlign="Center">
                                        <layoutData>
                                            <FlexItemData growFactor="3" />
                                        </layoutData>
                                    </Text>
                                </items>
                        </FlexBox>


                    </l:content>
                    </l:VerticalLayout>
                </CustomListItem>




