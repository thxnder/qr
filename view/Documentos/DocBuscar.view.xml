<Page backgroundDesign="Solid" showHeader="false">
    <BusyDialog id="loadingControl" />

                    <Panel>
                    <List    mode="SingleSelectMaster" growing="false" growingThreshold="30" growingDirection="Upwards" busyIndicatorDelay="200"
